#define  _GNU_SOURCE
#include <stdio.h>
#include <stdlib.h>
#include <commons/pcb.h>
#include <commons/collections/list.h>
#include <commons/serialization.h>
static t_seg_pila *crear_nodo_pila(int32_t linea, char *funcion);

double rafaga_estimada;
double rafaga_real;
double valor_alfa;
int16_t puertoHost;

int main (void){
	char* codigo;
	t_file_package *paq;
	t_pcb *pcb=malloc(sizeof(t_pcb));
	t_seg_pila * nodp;
	nodp = crear_nodo_pila(23,"una linea loca");
	int tam=load_file_to_memory("/home/utnso/test2.ansisop",&codigo);
	if(tam<10)printf("errrrrorrrr");
	paq = create_package_file(12,12,codigo);
	list_add(pcb->seg_pila,nodp);
	free(codigo);
	cargar_pcb(paq,pcb);
	free_package_file(paq);
destruir_pcb(pcb);

return 0;

}



static t_seg_pila *crear_nodo_pila(int32_t linea, char *funcion) {
	t_seg_pila *new = malloc(sizeof(t_seg_pila));
	new->llamada_funcion = strdup(funcion);
	new->linea = linea;
	return new;
}
